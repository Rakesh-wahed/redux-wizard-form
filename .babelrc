{
  "ignore": ["node_modules/**"],
  "presets": [["es2015", { "modules": false }], "react", "flow"],
  "plugins": ["transform-class-properties", "transform-object-rest-spread"],
  "env": {
    "development": {
      "plugins": ["external-helpers"]
    },
    "production": {
      "plugins": ["external-helpers"]
    },
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
